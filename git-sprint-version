GITSPRINT_VERSION=0.0.1-dev

initialize() {
    # A function can not be empty. Comments count as empty
    local FOO=''
}

usage() {
    OPTIONS_SPEC="\
git sprint version

Shows version information.

For more specific help type the command followed by --help
--
"
    flags_help
}

cmd_default() {
    echo "$GITSPRINT_VERSION"
}

cmd_help() {
    usage
    exit 0
}